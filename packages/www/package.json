{
  "name": "@zhquiz/www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node -r tsconfig-paths/register server-dist/index.js",
    "dev": "run-p 'dev:*'",
    "dev:nuxt": "nuxt dev",
    "dev:server": "docker-compose up",
    "build": "run-s 'build:*'",
    "build:nuxt": "nuxt build",
    "build:server": "tsc -P server/tsconfig.json",
    "lint": "eslint --ext '.js,.ts,.vue' --ignore-path .gitignore .",
    "ts": "ts-node -r tsconfig-paths/register -O '{\"module\":\"commonjs\",\"noImplicitAny\":false}'",
    "openapi": "wait-on tcp:$SERVER_PORT && curl -s http://localhost:$SERVER_PORT/api/doc/json -o ./assets/openapi.json && typegen ./assets/openapi.json > ./types/openapi.d.ts"
  },
  "dependencies": {
    "@databases/pg": "^5.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@magic-sdk/admin": "^1.3.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/pwa": "^3.3.5",
    "array-shuffle": "^2.0.0",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.2",
    "chinese-to-pinyin": "^1.3.1",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.4",
    "fastify": "^3.14.0",
    "fastify-csrf": "^3.0.1",
    "fastify-express": "^0.3.2",
    "fastify-rate-limit": "^5.5.0",
    "fastify-secure-session": "^2.3.1",
    "fastify-swagger": "^4.4.2",
    "jsonschema-definer": "^1.3.2",
    "lokijs": "^1.5.11",
    "magic-sdk": "^4.2.1",
    "node-gtts": "^2.0.2",
    "nodejieba": "^2.5.1",
    "nuxt": "^2.15.3",
    "nuxt-buefy": "^0.4.4",
    "nuxt-property-decorator": "^2.9.1",
    "openapi-client-axios": "^3.13.1",
    "short-uuid": "^4.1.0",
    "tsconfig-paths": "^3.9.0",
    "wait-on": "^5.3.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/better-sqlite3": "^5.4.1",
    "@types/lokijs": "^1.5.3",
    "@types/wait-on": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "import-sort-parser-typescript": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nuxt-typed-vuex": "^0.1.22",
    "openapi-client-axios-typegen": "^3.13.1",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "10",
    "ts-node": "^9.1.1",
    "vue-context": "^6.0.0"
  },
  "importSort": {
    ".js, .ts": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
